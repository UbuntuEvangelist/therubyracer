<%= if order.ord_type == 'limit' %>
  <td><%= order.price %></td>
    <%= else %>
  <td><%= order.ord_type %></td>
    <%= end %><tr>
  <td><%= order.id %></td>
  <td><%= t(".#{order.kind}") %></td>
  <td><%= t(".#{order.state}") %></td>
  <td><%= order.currency.upcase %></td>
  <td><%= order.price %></td>
  <td><%= order.origin_volume %></td>
  <td><%= order.avg_price %></td>
  <td><%= order.origin_volume - order.volume %></td>
  <td><%= order.created_at %></td>
</tr>
