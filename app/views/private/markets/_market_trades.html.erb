<div id="market_trades_wrapper" class="has-right-dropdown">
  <div class="trade_market_hdr">
    <div class="trade_market_left">
      <h2><%= t('.trade_history')%></h2>
    </div>
    <div class="trade_market_right">
      <ul>
        <% if current_user %>
          <li><a class="all active" href="javascript:void(0)"><%= t('.all_html') %></a></li>
          <li><a class="my" href="javascript:void(0)"><%= t('.my_html') %></a></li>
        <% end %>
      </ul>
    </div>
  </div>
  <div id="market_trades">
    <!--id="scroll_trade_market" for scrollbar in trade_market-->
      <div class="trade_market" id="scroll_trade_market">
        <table class="table table-hover all-trades">
          <tbody></tbody>
        </table>
        <% if current_user %>
          <table class="table table-hover my-trades">
            <tbody></tbody>
          </table>
        <% end %>
      </div>
  </div>
</div>
