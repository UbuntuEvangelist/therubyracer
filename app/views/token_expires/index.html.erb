<% content_for :header do %> <span></span>
<% end %>
<% content_for :footer do %> <span></span>
<% end %>
<section class="expire_email_token">
  <div class="container">
    <div class="login_block">
      <div class="login_block_inner signup_block">
        <div class="login_logo"><a class="navbar-brand top_logo" href="<%= root_path %>"></a></div>
        <div class="login_hding">
          <span>Account Activation</span>
        </div>
        <div class="middle ng-scope">
          <div class="messageBox-con">
            <% if params[:flag].eql?("true") %>
                <p class="ng-binding"><%= t('.email_activation') %>&nbsp;
                  <% if !current_user.present? %>
                      <a href="<%= signin_path %>" class="ng-binding login_now">Log In</a></p>
                  <% end %>
            <% else %>
                <p class="ng-binding"><%= t('.email_activation') %>
                  <a href="<%= signin_path %>" class="ng-binding login_now">Log In</a></p>
            <% end %>
          </div>
        </div>
      </div>

    </div>
  </div>
  </div>
</section>

