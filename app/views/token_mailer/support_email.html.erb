<table style="font-size:14px;font-family:Microsoft Yahei,Arial,Helvetica,sans-serif;padding:0;margin:0;color:#333;background-image:none;background-color:#f7f7f7;background-repeat:repeat-x;background-removed-by-webmail:bottom left" width="100%" height="100%" cellspacing="0" cellpadding="0" border="0">
  <tbody>
  <tr>
    <td>
      <table width="600" cellspacing="0" cellpadding="0" border="0" align="center">
        <tbody>
        <tr>
          <td style="padding:33px 0" valign="middle" align="center">
            <a href="<%="#{ENV.fetch('URL_SCHEME', 'http')}://#{ENV['URL_HOST']}"%>" target="_blank" defaultcontextmenu="yes"></a></td>
        </tr>
        <tr>
          <td>
            <div style="padding:0 30px;background:#fff">
              <table width="100%" cellspacing="0" cellpadding="0" border="0">
                <tbody>
                <tr>
                  <td style="border-bottom:1px solid #e6e6e6;font-size:18px;padding:20px 0">
                    <table width="100%" cellspacing="0" cellpadding="0" border="0">
                      <tbody>
                      <tr>
                        <td style="color: #333;">Email From user</td>
                        <td>

                        </td>
                      </tr>
                      </tbody>
                    </table>
                  </td>
                </tr>
                <tr>
                  <td style="font-size:14px;line-height:30px;padding:20px 0;color:#666"><p>  You got Mail form <%= @support.name %> , Message  of user is:<br> <%= @support.body %> and details is below</p>
                  </td>
                </tr>
                <tr>
                  <td style="padding:20px 0 10px 0;line-height:26px;color:#666"><p> email of user is:<br> <%= @support.email %></p>
                  </td>
                </tr>
                <tr>
                  <td style="padding:20px 0 10px 0;line-height:26px;color:#666"><p> Contact number  of user is:<br> <%= @support.contact_no %></p>
                  </td>
                </tr>
                <tr>
                  <td style="padding:20px 0 10px 0;line-height:26px;color:#666"> <p> website  or url  is:<br> <%= @support.url %></p>
                  </td>
                </tr>
                </tr>
                </tbody>
              </table>
            </div>
          </td>
        </tr>
        <td style="font-size:12px;color:#999;padding:20px 0" align="center">©  <%= (Time.now.year)-1 %>- <%= Time.now.year %> <%="#{ENV.fetch('URL_SCHEME', 'http')}://#{ENV['URL_HOST']}"%> All Rights Reserved<br>URL： <a style="color:#999;text-decoration:none" href=" <%="#{ENV.fetch('URL_SCHEME', 'http')}://#{ENV['URL_HOST']}"%> " target="_blank" defaultcontextmenu="yes"> <%="#{ENV.fetch('URL_SCHEME', 'http')}://#{ENV['URL_HOST']}"%> </a>&nbsp;&nbsp; E-mail： <a href="<%= raw t("mailer.please_contact", contact_mail: ENV['SUPPORT_MAIL']) %>" style="color:#999 ;text-decoration:none" target="_blank" defaultcontextmenu="yes"> <%= ENV['ADMIN']%> </a>
        </td>
        </tr>
        </tbody>
      </table>
    </td>
  </tr>
  </tbody>
</table>



